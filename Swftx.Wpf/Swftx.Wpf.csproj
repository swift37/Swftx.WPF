<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0-windows</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

  <PropertyGroup>
    <Version>1.0.0</Version>
    <PackageReleaseNotes>
      Project launch
    </PackageReleaseNotes>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>Swftx.WPF</PackageId>
    <Authors>swift37</Authors>
    <Description>A set of tools for working with the MVVM design pattern in WPF.</Description>
    <Copyright>rafchik37@gmail.com</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/swift37/Swftx.WPF</RepositoryUrl>
    <PackageProjectUrl>https://github.com/swift37/Swftx.WPF</PackageProjectUrl>
    <PackageTags>WPF;XAML;.Net;MVVM;Toolkit</PackageTags>
    <SignAssembly>True</SignAssembly>
    <AssemblyOriginatorKeyFile>Swftx.Wpf.snk</AssemblyOriginatorKeyFile>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

</Project>
